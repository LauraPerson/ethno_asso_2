<div class="container">
  <%= simple_form_for((@partner), data: {turbo: false}) do |f| %>
    <%= f.input :name, placeholder: "Nom du partenaire", label: false  %>
    <%= f.input :photo, as: :file, label:"Modifier l'image" %>
    <%= f.submit "Modifier ce partenaire", class:"btn-primary"%>
  <% end %>
</div>
