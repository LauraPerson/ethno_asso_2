
<div class="container">
  <div class="d-flex justify-content-between border-bottom my-4">
    <h3 class="d-none-xs">Tableau de bord</h3>
    <h3>Ajouter un partenaire</h3>
  </div>
  <%= simple_form_for(@partner) do |f| %>
    <%= f.input :name, label: "Nom du partenaire" %>
    <%= f.input :url, label: "Lien URL du partenaire" %>
    <%= f.input :photo, as: :file, label: "Ajouter le logo du partenaire" %>
    <%= f.submit "Ajouter un partenaire", class:"btn-primary" %>
  <% end %>
</div>