
<div class="container-partners">
  <div class="container">
    <div class="d-flex my-3"><%= image_tag "star.png", width: "60px", height:"60px", class:"mx-2" %><h2 class="my-3">Nos partenaires</h2></div>
    <div class="card-grid" id="card-grid-partner">
      <% @partners.each do |partner| %>
        <div class="card-partner">
          <div class="card-partner-img">
            <%= unique_photo(partner) %>
          </div>
          <h3><%= partner.name %></h3>
          <% if current_user.super_admin %>
            <%= button_to( "Supprimer", partner_path(partner), method: :delete, class:"btn-secondary mx-2") %>
          <% end %>
        </div>
      <% end %>
    </div>

  </div>
</div>